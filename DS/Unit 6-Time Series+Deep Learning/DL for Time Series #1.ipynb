    "import pandas as pd\n",
    "import numpy as np\n",
    "seed = 2016\n",
    "np.random.seed(seed)\n",
    "y_0 = 1\n",
    "alpha = -0.25\n",
    "beta = 0.95\n",
    "y = pd.Series(y_0)\n",
    "y\n",
    "num=10\n",
    "for i in range(num):\n",
    "    y_t = alpha + ( beta * y_0 ) + np.random.uniform(-1, 1)\n",
    "    y = y.set_value(i, y_t)\n",
    "    y_0 = y_t"
   ]
